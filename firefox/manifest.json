{
  "manifest_version": 3,
  "name": "JSXtract",
  "version": "1.0",
  "description": "Get data from a web apps JS files using many forms of regex.",
  "permissions":[
    "activeTab",
    "tabs",
    "scripting"
  ],
  "host_permissions":[
    "<all_urls>"
  ],
  "action":{
    "default_popup": "popup.html"
  }
}
